# Define server logic
server <- function(input, output) {
# Load or create data frame
data <- data.frame(x = rnorm(100), y = rnorm(100), z = rnorm(100))
# Define reactive variable for selected variables
selected_variables <- reactive({
c(input$variable_x, input$variable_y)
})
# Plot data based on selected variables
output$plot <- renderPlot({
# Filter data to include selected variables only
filtered_data <- data[, selected_variables()]
# Create plot using ggplot2
ggplot(filtered_data, aes(x = filtered_data[[1]], y = filtered_data[[2]])) +
geom_point() +
labs(
title = "Scatter Plot",
x = paste0("X Variable (", input$variable_x, ")"),
y = paste0("Y Variable (", input$variable_y, ")")
)
})
}
